[github]
	user = pizzaops
[core]
	editor = vim
	autocrlf = false
	safecrlf = true
	filemode = false
	excludesfile = /Users/ZAlexander/.gitignore_global
[user]
	name = ZoÃ« Alexander
	email = zoe.alexander@hunterstrategy.net
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  lc =  log --pretty=format:'%h %ad| %s%d [%an]' --graph --date=short --max-count=1
[push]
	default = simple
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[merge]
	ff = only
