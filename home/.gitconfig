[github]
	user = zacharyalexstern
[core]
	editor = vim
	autocrlf = input
	safecrlf = true
	filemode = false
[user]
	name = Zachary Alex Stern
	email = zacharyalexstern@gmail.com
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  lc =  log --pretty=format:'%h %ad| %s%d [%an]' --graph --date=short --max-count=1
[push]
	default = simple
